{
  "name": "tsbi",
  "version": "1.0.0",
  "description": "tsbi 是一个使用 TypeScript 实现的大数运算库，可在各种环境上启用公钥加密。",
  "author": "RSM <RSoraM@hotmail.com>",
  "homepage": "https://github.com/RSoraM/tsbi",
  "repository": {
    "type": "git",
    "url": "git@github.com:RSoraM/tsbi.git"
  },
  "license": "MIT",
  "keywords": [
    "tsbi",
    "TypeScript",
    "BigIntegers"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./src/index.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild",
    "release": "standard-version && npm publish",
    "test": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "nr build",
    "publish": "publish"
  },
  "devDependencies": {
    "@antfu/ni": "^0.20.0",
    "@vitest/ui": "^0.29.2",
    "git-cz": "^4.9.0",
    "prettier": "^2.8.4",
    "standard-version": "^9.5.0",
    "typescript": "^4.9.5",
    "unbuild": "^1.1.2",
    "vitest": "^0.29.2"
  }
}
